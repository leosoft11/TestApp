{
    "manifest_version": 2,
    "name": "Console Errors",
    "description": "Base",
    "version": "0.0.1",
    "author": "lukarek",
    "browser_action": {
        "default_title": "Поиск ошибок в консоле",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["main.js", "main1.js"]
        }
    ],
    "permissions": ["activeTab"]
}